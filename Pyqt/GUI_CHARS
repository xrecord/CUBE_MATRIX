    def BACK(self):
        array_ind.pop()
        array_value.pop()
    def SAVE_STROKE(self,i,j):
        #self.setDisabled(1)
        if j == 0:
            mask=0b01111111
        elif j ==1:
            mask=0b10111111
        elif j ==2:
            mask=0b11011111
        elif j ==3:
            mask=0b11101111
        elif j ==4:
            mask=0b11110111
        elif j ==5:
            mask=0b11111011
        elif j ==6:
            mask=0b11111101
        elif j ==7:
            mask=0b11111110
        array[i]=(array[i]&mask)
        array_ind.append(i)
        array_value.append(array[i])
